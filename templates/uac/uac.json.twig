{
    "version": 1,
    "routes": {
        "common.access": {
            "common.start_dashboard": {
                "access": true,
                "routes": [
                    "dashboard",
                    "appConfig",
                    "gridExcel"
                ]
            }
        },
        {% include '/uac/machines.json.twig' %},
        "report.all": {
        {% for rep in reports %}
            "{{rep.NAME}}": {
                "access": {{rep.ROLE_ID == null ? 'false' : 'true'}},
                "routes": ["{{rep.ID}}"],
                "typeNode": "report"
            }
            {{loop.last ? '' : ','}}
        {% endfor %}
        }
    },
    "widgets": []
}
